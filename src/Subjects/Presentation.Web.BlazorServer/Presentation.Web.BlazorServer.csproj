<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<RootNamespace>Goodtocode.Subjects.BlazorServer</RootNamespace>
		<AssemblyName>Goodtocode.Subjects.BlazorServer</AssemblyName>
		<Version>1.0.0</Version>
		<TargetFramework>net7.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<UserSecretsId>aspnet-Goodtocode.Subjects.BlazorServer-162fe164-2aae-4a7b-adcf-a4ef2cd6803d</UserSecretsId>
		<WebProject_DirectoryAccessLevelKey>0</WebProject_DirectoryAccessLevelKey>
	</PropertyGroup>

	<ItemGroup>
	  <Content Remove="wwwroot\favicon.png" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="wwwroot\favicon.png" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Azure.Extensions.AspNetCore.Configuration.Secrets" Version="1.2.2" />
		<PackageReference Include="BlazorPro.Spinkit" Version="1.2.0" />
		<PackageReference Include="Microsoft.ApplicationInsights.AspNetCore" Version="2.21.0" />
		<PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="7.0.5" NoWarn="NU1605" />
		<PackageReference Include="Microsoft.AspNetCore.Authentication.OpenIdConnect" Version="7.0.5" NoWarn="NU1605" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0" />
		<PackageReference Include="Microsoft.Identity.Web" Version="2.11.1" />
		<PackageReference Include="Microsoft.Identity.Web.UI" Version="2.11.1" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Common\Common.Infrastructure.ApiClient\Common.Infrastructure.ApiClient.csproj" />
	  <ProjectReference Include="..\Infrastructure.Persistence\Infrastructure.Persistence.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Content Update="Pages\Business\BusinessCreate.razor">
	    <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
	  </Content>
	</ItemGroup>

</Project>
